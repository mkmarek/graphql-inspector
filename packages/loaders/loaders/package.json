{
  "name": "@graphql-inspector/loaders",
  "version": "3.4.0",
  "description": "Loaders in GraphQL Inspector",
  "repository": {
    "type": "git",
    "url": "kamilkisiela/graphql-inspector",
    "directory": "packages/plugins/plugin"
  },
  "author": {
    "name": "Kamil Kisiela",
    "email": "kamil.kisiela@gmail.com",
    "url": "https://github.com/kamilkisiela"
  },
  "license": "MIT",
  "main": "dist/index.js",
